---
import About from '../components/About.astro';
import ContactSection from '../components/ContactSection.astro';
import Hero from '../components/Hero.astro';
import Navbar from '../components/Navbar.astro';
import Layout from '../layouts/Layout.astro';

const title = 'Code, Read, Succeed Blog';
const desc =
  "Welcome to Code, Read, Succeed! This is your go-to blog for a blend of coding insights, productivity hacks, book summaries, and personal reflections. Whether you're looking to enhance your coding skills, find new ways to boost your productivity, or discover inspiring books, you'll find valuable content here";
---

<Layout title={title} description={desc}>
  <Navbar />
  <Hero />
  <About />
  <ContactSection />
</Layout>
